{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "outDir": "./dist",
    "noEmit": true,
    "strict": false,
    "noImplicitAny": false,
    "allowUnreachableCode": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "suppressExcessPropertyErrors": true
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    "node_modules",
    "dist",
    "main.js",
    "index.html",
    "style.css",
    "processor.js",
    "nexus-dsp.cpp"
  ]
}
